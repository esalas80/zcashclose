<mvc:View
    controllerName="NAMESPACE.zcashclose.controller.QueryPrinter"
		height="100%"
		xmlns:core="sap.ui.core"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:semantic="sap.m.semantic"
		xmlns:form="sap.ui.layout.form"
		xmlns:ui="sap.ca.ui"
		displayBlock="true">

    <semantic:FullscreenPage title="FullScreen Page Title" showNavButton="true" navButtonPress="onNavBack">
        <semantic:content>
            <ObjectHeader title="Type something in the field, the Draft Indicator will be displayed in the footer"
                intro="this is just a simulation of how the DraftIndicator will work"
                responsive="true"/>
            <form:SimpleForm editable="true">
                <Label text="Type here"/>
                <Input id="TypeHere" liveChange="handleLiveChange"/>
            </form:SimpleForm>
        </semantic:content>
    </semantic:FullscreenPage>
</mvc:View>